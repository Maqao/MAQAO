TEXT = {}

-- Create the class meta
TEXTMeta         = {}
TEXTMeta._NAME   = "TEXT"
TEXTMeta.__index = TEXT

function TEXT:new()
   local text = {};
   setmetatable(text,TEXTMeta);
   
   text.numbered = true
   text.header_1 = 0
   text.header_2 = 0
   text.header_3 = 0
   text.header_4 = 0
   text.header_5 = 0
   
   return text;
end

dofile ("@LUA_SCRIPTS_PATH@/built_in/classes/TEXT/text.lua")